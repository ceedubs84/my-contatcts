<div>
  
</div>
<h1>Contact List</h1>
<% @contacts.each do |contact| %>
  <div class="well">
    <h2><a href="/contacts/<%= contact.id %>"><%= contact.full_name %></a></h2>
    <h4>First Name: <%= contact.first_name %></h4>
    <h4>Middle Name: <%= contact.middle_name %></h4>
    <h4>Last Name: <%= contact.last_name %></h4>
    <h4>Email: <%= contact.email %>
    <h4>Bio: <%= contact.bio %>
    <h4>Phone Number: <%= contact.phone_number %></h4>
  </div>  
<% end %>

<div>
  <a class="btn btn-success" href="/contacts/new" role="button">Create New Contact</a>
</div>